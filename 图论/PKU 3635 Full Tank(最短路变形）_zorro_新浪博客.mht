                if (ss[i].src && ss[i].src.indexOf(url) !=3D -1) {
                    head.removeChild(ss[i]);
                }
            }
            var s =3D document.createElement("script");
            s.type =3D "text/javascript";
            s.src =3D url;
            s.charset =3D charset ? charset : "utf-8";
            head.appendChild(s);
            var self =3D this;
            s.onload =3D s.onreadystatechange =3D function(){
                if (this.readyState && this.readyState =3D=3D "loading")
                    return;
                self.onsuccess();
            };
            s.onerror =3D function(){
