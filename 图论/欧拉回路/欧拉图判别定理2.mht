	dian=temp.substring(6,8);
	ye=temp.substring(9,11);
	index=1;
	if(zhang=="14")
	{
		index+=0;
		if(jie=="01")index+=0;
		else if(jie=="02")index+=11;
		else if(jie=="03")index+=14;
		else if(jie=="04")index+=21;
		else if(jie=="05")index+=25;
		else if(jie=="06")index+=26;
	}
	else if(zhang=="15")
	{
		index+=27;
		if(jie=="01")index+=0;
		else if(jie=="02")index+=5;
		else if(jie=="03")index+=9;
		else if(jie=="04")index+=11;
	}	
	else if(zhang=="16")
	{
		index+=39;
		if(jie=="01")index+=0;
		else if(jie=="02")index+=3;
		else if(jie=="03")index+=8;
		else if(jie=="04")index+=13;
	}
	else if(zhang=="17")	
	{
		index+=53;
		if(jie=="01")index+=0;
		else if(jie=="02")index+=4;
		else if(jie=="03")index+=7;
		else if(jie=="04")index+=12;
		else if(jie=="05")index+=16;
		else if(jie=="06")index+=18;
		else if(jie=="07")index+=20;
		else if(jie=="08")index+=23;
	}
	else if(zhang=="18")
	{
		index+=77;
		if(jie=="01")index+=0;
		else if(jie=="02")index+=3;
		else if(jie=="03")index+=7;
		else if(jie=="04")index+=11;
	}
	index+=dian*1-1;
	
	if(flag)
	{
		ye=ye*1+1;
		if(ye>page[index]){index++;ye=1;}
	}
	else{
		ye=ye*1-1;
		if(ye<=0){index--;ye=page[index];}
	}
	if(index<=0)parent.location.href="../part3/main.htm";
	else if(index==100)parent.location.href="../part4/main.htm";
	else{
		tmp=str[index];
		zh=tmp.substring(0,2);
		//parent.leftFrame.document.writeln(tmp);
