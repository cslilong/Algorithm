        		messagereq =3D new XMLHttpRequest();
        		messagereq.onreadystatechange =3D callback;
        		messagereq.open("GET", url);
        		messagereq.send(null);
    		} // branch for IE/Windows ActiveX version
    		else if (window.ActiveXObject) {
        		messagereq =3D new ActiveXObject("Microsoft.XMLHTTP");
       		if (messagereq) {
            		messagereq.onreadystatechange =3D callback;
            		messagereq.open("GET", url, false);
            		messagereq.send();
        		}
    		}
    }
    catch(ex){
